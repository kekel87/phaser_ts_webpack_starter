{
  "name": "phaser_ts_webpack_starter",
  "version": "1.0.0",
  "description": "Pack de d√©part pour phaser + typescript + webpack",
  "main": "src/Game.ts",
  "scripts": {
    "start": "npm run server",
    "server": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.server.js",
    "server:reload": "npm run server -- --inline --hot",
    "build": "npm run build:prod",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.build.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.config.build.js",
    "lint": "./node_modules/.bin/tslint 'src/**/*.ts'"
  },
  "author": "Michael PARRY<michael.parry.87@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/easystarjs": "^0.1.29",
    "@types/node": "^6.0.51",
    "autoprefixer": "^6.3.6",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.0",
    "easystarjs": "^0.2.3",
    "expose-loader": "^0.7.1",
    "ignore-styles": "^5.0.1",
    "json-loader": "^0.5.4",
    "p2": "^0.7.1",
    "phaser": "^2.4.7",
    "phaser-debug": "^1.1.8",
    "phaser-plugin-isometric": "git+https://github.com/kekel87/phaser-plugin-isometric.git",
    "postcss-import": "^8.1.0",
    "postcss-loader": "^1.1.1",
    "precss": "^1.4.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.2.2",
    "tslint": "^4.0.2",
    "tslint-loader": "^3.2.0",
    "typescript": "^2.0.10",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-fail-plugin": "^1.0.5",
    "webpack-merge": "^0.18.0"
  }
}
